<% content_for :title, "#{ @klass_str }>" %>

<h1><%= @klass_str %></h1>

<%= render  @object %>

<br>

<div class='ov-button-row'>
<%= ov_edit @object %>
<%= ov_index @klass %>
<%= ov_delete @object %>
</div>
<h2>Projection</h2>
<div>
  <% @proj = @object.project %>
</div>
<%= render "projection_graph",
    title: "#{@asset.name} #{@asset.type_title}",
    label: "#{@asset.name}/#{@asset.type_title}"%>
<%= render "projection_table" %>
